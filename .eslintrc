{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "no-debugger": 0,
    "no-unexpected-multiline": 2,
    "indent": ["error", 2],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "curly": 2,
    "eqeqeq": [2, "smart"],
    "no-extend-native": 2,
    "guard-for-in": 2,
    "wrap-iife": [2, "inside"],
    "no-caller": 2,
    "no-eval": 2,
    "no-inner-declarations": [2, "both"],
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-shadow-restricted-names": 2,
    "new-parens": 2,
    "space-before-function-paren": [2, { "anonymous": "always", "named": "never" }],
    "keyword-spacing": 2,
    "no-labels": 2,
    "block-spacing": [2, "always"],
    "space-before-blocks": 2,
    "space-infix-ops": 2,
    "comma-spacing": [2, { "before": false, "after": true }],
    "array-bracket-spacing": [2, "never"],
    "space-unary-ops": [1, { "words": true, "nonwords": false }],
    "semi-spacing": [2, {"before": false, "after": true}],
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "dot-notation": 2,
    "no-lone-blocks": 2,
    "no-redeclare": 2,
    "no-with": 2,
    "global-require": 0,
    "comma-style": [2, "last"],
    "computed-property-spacing": [2, "never"],
    "no-array-constructor": 2,
    "no-multiple-empty-lines": 2,
    "no-spaced-func": 2,
    "object-curly-spacing": [2, "always"],
    "space-in-parens": [2, "never"],
    "arrow-spacing": 2
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "commonjs": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "jQuery": true,
    "__VERSION__": true,
    "nidCommandQueueNamespace": true
  }
}
